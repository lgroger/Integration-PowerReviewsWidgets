  var google_conversion_format = "{{ themeSettings.gConvFormat }}";
  var google_conversion_color = "{{ themeSettings.gConvColor }}";
  var google_conversion_label = "{{ themeSettings.gConvLabel }}";
  var google_conversion_currency = "{{ themeSettings.gConvCur }}";
  var google_conversion_value = {{ model.total|floatformat(2) }};
  var google_remarketing_only = {{ themeSettings.gRemarketing|lower }};
  /* ]]> */
  </script>
  <script type="text/javascript" src="//www.googleadservices.com/pagead/conversion.js">
  </script>
