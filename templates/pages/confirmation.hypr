<div class="print_header" style="display:none;">
  <div class="logo_box">
    <img src="/resources/images/Logo_Shindigz.png" height="60px">
  </div>
  <div class="text_box">
      {% if model.billingInfo.paymentType == 'Check' %}
          Your quote number is <span>{{ model.orderNumber }}</span>
      {% else %}
          Your order number is <span>{{ model.orderNumber }}</span>
      {% endif %}
      TOTAL: <span>{{ model.total|currency }}</span>
  </div>
</div>
<div class="mz-l-container order-confirmation">
    <h1 class="mz-pagetitle">
        {% if model.billingInfo.paymentType == 'Check' %}
          {{ labels.quoteConfirmation }}
          <span class="print-link" onClick="window.print()"><u>Print Receipt</u></span>
        {% else %}
          {{ labels.orderConfirmation }}
          <span class="print-link" onClick="window.print()"><u>Print Receipt</u></span></h1>
        {% endif %}
    </h1>
    <div class="mz-cms-col-12-12 confrim-block">
        <div class="icon-block mz-cms-col-8-12">
            <div class="tick">
                <i class="icon fa fa-check" aria-hidden="true"></i>
            </div>
            {% if model.billingInfo.paymentType == 'Check' %}
                <h1 class="confirm-text1">{{ labels.quoteconfirmationText }}</h1>
            {% else %}
                <h1 class="confirm-text1">{{ labels.confirmationText }}</h1>
            {% endif %}
            <!-- <h1 class="confirm-text1">Thank you for your order {{ model.billingInfo.billingContact.firstName }}!</h1> -->
            <h3 class="confirm-text2">You will receive an email confirmation</h3>
        </div>
        <div class="mz-cms-col-4-12 total-sec">
            <div class="payment-sec">
            {% if model.billingInfo.paymentType == 'Check' %}
                <h4 class="order-det1">Your quote number is <span>{{ model.orderNumber }}</span></h4>
            {% else %}
                <h4 class="order-det1">Your order number is <span>{{ model.orderNumber }}</span></h4>
            {% endif %}
            <h2 class="order-det2">TOTAL PAYMENT: <span>{{ model.total|currency }}</span></h2>
            </div>
        </div>
    </div>
     <div class="clear"></div>
     {% if model.hasDirectShip %}
    <div class="shipping-info">
      <h3 class="shipping-head">Shipping Information</h3>
      <hr>
      <div class="div_33">
        <div class="block1">
           <span class="ship-text1" >{{ labels.shipTo }}:</span>
        </div>
        <div class="block2">
          <div class="confirmation_address">
            {% include "modules/common/confirmation-address" with model=model.fulfillmentInfo.fulfillmentContact %}
          </div>
        </div>
      </div>
      <div class="div_66">
        <div class="block1">
            <span class="ship-text1"> {{ labels.shippingMethod }}:</span>
        </div>
        <div class="block2">
            <span class="ship-text2">{{ model.fulfillmentInfo.shippingMethodName }}</span>
        </div>
      </div>
      <div class="clear"></div>
      <hr>
    </div>
    {% endif %}

    <div class="shipping-info billing-info">
      <h3 class="shipping-head">Billing Information</h3>
      <hr>
      <div class="div_33">
        <div class="block1">
           <span class="ship-text1" >{{ labels.billingEmail }}:</span>
        </div>
        <div class="block2">
            <span class="ship-text2">{{ model.billingInfo.billingContact.email }}</span>
        </div>
      </div>
      <div class="div_33 billing-block">

        <div class="block2">
          {% for addresspayment in model.payments %}
             {% if addresspayment.status != "Voided" and addresspayment.status != "Declined" %}
              {% include "modules/checkout/billing-info" with model=addresspayment %}
              {% endif %}
          {% endfor %}

        </div>
      </div>
      <div class="div_33">
        <div class="block1">
            <span class="ship-text1 ">Payment Method:</span>
        </div>
         <div class="block2">
            <span class="ship-text2 ">
                <span>
                    {% for payment in model.payments %}
                        {% if payment.status != "Voided" and payment.status != "Declined" %}
                                <span class="paymentstatus" id="GTMpaymentstatus"> {# mapped to PaymentStatusInConfirmationPage in GTM #}
                                    {% include "modules/checkout/checkout-payment" with model=payment %}
                                </span>
                        {% endif %}
                    {% endfor %}
                </span>
            </span>
        </div>


      </div>
      <div class="clear"></div>
      <hr>
    </div>

    <div class="confirm_orders">
      <div class="orders-sec">
        <h3 class="orders-heading">Order Summary</h3>
        <hr>
          <div class="orders mz-cms-col-8-12">
            <div class="confirmation_ostable">
              <table class="mz-ordersummary">
                <thead>
                    <tr>
                        <th class="mz-ordersummary-header-product">{{ labels.item }}</th>
                        <th class="mz-ordersummary-header-description"><span class="mob_hide">{{ labels.fullDesc }}</span></th>
                        <th class="mz-ordersummary-header-each">{{ labels.price }}</th>
                        <th class="mz-ordersummary-header-qty"><span class="mob_hide">{{ labels.quantity }}</span><span class="mob_show">{{ labels.qty }}</span></th>
                        <th class="mz-ordersummary-header-total">{{ labels.subtotal }}</th>
                    </tr>
                </thead>
                {% for item in model.items %}
                    {% with forloop.counter0 as idx %}
                    <tbody class="mz-ordersummary-lineitems">
                      <tr class="mz-ordersummary-line mz-ordersummary-line-item {% if item.discountTotal > 0 %}is-discounted{% endif %}">
                            <td class="mz-ordersummary-item-image">
                              {% if item.product.imageUrl and themeSettings.showCartItemImage %}
                                <div class="cp_image">
                                  {% if item.product.options and item.product.productType != "Bundle" %}
                                    {% set_var hasDND=false %}
                                    {% for option in item.product.options %}
                                      {% if option.attributeFQN == themeSettings.productAttributes.dndToken %}
                                          {% set_var hasDND=true %}
                                          {# trying to escape quote with single quote of dndtoken but I can only seem to replace it with ! for some reason so we'll have to undo it in javscript #}
                                            <img src="{% make_url "image" item.product with max=80 as_parameter %}" alt="{{item.product.name}}" data-fulldndtoken='{{option.shopperEnteredValue|replace('"','!')|escape}}'>
                                      {% endif %}
                                    {% endfor %}
                                    {% if hasDND==false %}
                                       <img class="mz-carttable-item-image" src="{% make_url "image" item.product with max=80 as_parameter %}" {% if item.product.imageAlternateText %}alt="{{ item.product.imageAlternateText }}"{% else %}alt="{{item.product.name}}"{% endif %} />
                                    {% endif %}
                                  {% else %}
                                  <img class="mz-carttable-item-image" src="{% make_url "image" item.product with max=80 as_parameter %}" {% if item.product.imageAlternateText %}alt="{{ item.product.imageAlternateText }}"{% else %}alt="{{item.product.name}}"{% endif %} />
                                  {% endif %}
                                </div>
                              {% endif %}
                            </td>
                            <td class="mz-ordersummary-item-product">
                                {{ item.product.name }}
                                <br>
                                {% for attr in model.attributes %}
                                  {% if attr.fullyQualifiedName == "tenant~est-delivery-date" %}
                                    {%  with attr.values.0|split(":::") as estd %}
                                      {% for est in estd %}
                                        {% if forloop.counter0 == idx %}
                                          <br> Delivered by: {{est}}
                                        {% endif %}
                                      {% endfor %}
                                    {% endwith %}
                                  {% endif %}
                                {% endfor %}
                                <div class="item_small_description">
                                {% if item.product.options.length > 0 %}
                                  {% for opt in item.product.options %}
                                    {% if opt.name=="dnd-token" && opt.attributeFQN =="tenant~misc.-favor-with-design" %}
                                      {# tenant~misc.-favor-with-design is for inventory purposes mostly, no need to show to customer #}
                                    {% else %}
                                          <p><span class="sd_label">{{ opt.name }}:</span><span class="sd_text">{{ opt.value }}</span></p>
                                    {% endif %}
                                  {% endfor %}
                                {% endif %}
                                </div>
                            </td>
                            <td class="mz-ordersummary-item-price">
                                {% include "modules/common/item-price" %}
                            </td>
                            <td class="mz-ordersummary-item-qty">{{ item.quantity }}</td>
                            <td class="mz-ordersummary-item-total">
                              {% include "modules/common/item-total" %}
                            </td>
                        </tr>

                        <tr class="mz-ordersummary-item-shipping-info">
                          <td colspan="5" class="mz-ordersummary-item-availability">
                          {% if item.product.productUsage == 'Bundle' %}
                              <h3><span class="toggle_components" data-parent-id="{{ item.id }}">Show Components <i class="fa fa-caret-down"></i></span></h3>
                               {% if item.product.options %}
                                    {% set_var hasDND=false %}
                                    {% for option in item.product.options %}
                                      {% if option.attributeFQN == themeSettings.productAttributes.dndToken %}
                                          {% set_var hasDND=true %}
                                          {# trying to escape quote with single quote of dndtoken but I can only seem to replace it with ! for some reason so we'll have to undo it in javscript #}
                                          <div class="hide_bundle_products"  data-parent-id="{{ item.id }}" style="display:none;" data-fulldndtoken-bundle='{{option.shopperEnteredValue|replace('"','!')|escape}}'>
                                      {% endif %}
                                    {% endfor %}
                                    {% if hasDND==false %}
                                       <div class="hide_bundle_products"  data-parent-id="{{ item.id }}" style="display:none;">
                                    {% endif %}
                                {% else %}
                                    <div class="hide_bundle_products"  data-parent-id="{{ item.id }}" style="display:none;">
                                {% endif %}



                              
                                  <div class="bundle_lists">
                                      {% for components in item.product.bundledProducts %}
                                        <div class="mz-cms-col-4-12 bundle_box">
                                            <div class="bb_labels">
                                                <h3 class="bundle_heading">
                                                  {{components.name}}
                                                </h3>
                                                <h3 class="bundle_quantity"><span>X</span>{{components.quantity}}</h3>
                                            </div>
                                            <img src="{% include_products "modules/product/bundle-component" with productCodes=components.productCode and pageWithUrl=true and sortWithUrl=true and pageSize=themeSettings.defaultPageSize as_parameter %}" alt="{{components.name}}" data-productcode="{{components.productCode}}" />
                                        </div>
                                      {% endfor %}
                                  </div>
                              </div>
                          {% endif %}
                          </td>
                        </tr>
                    </tbody>
                    {% endwith %}
                {% endfor %}
              </table>
            </div>
          </div>
          <div class="cart-rec mz-cms-col-4-12">
            <h2 class="cart_summary_title">Cart Summary</h2>
            <div class="confirmation_ostable_total">
                <table class="mz-ordersummary">
                    <tbody>
                        <tr class="mz-ordersummary-subtotal">
                          <td><span class="mz-ordersummary-totalname">{{ labels.subtotal }}</span></td>
                          <td><span class="mz-ordersummary-totalvalue">{{ model.discountedSubtotal|currency }}</span></td>
                        </tr>

                        <tr class="mz-ordersummary-shippingtotal">
                          <td><span class="mz-ordersummary-totalname">Shipping</span></td>
                          <td><span class="mz-ordersummary-totalvalue">{{ model.shippingSubTotal|currency }}</span></td>
                        </tr>

                        {% for shippingDiscount in model.shippingDiscounts %}
                        <tr class="mz-ordersummary-shippingdiscount">
                          <td><span class="mz-ordersummary-totalname">Shipping Discount</span></td>
                          <td><span class="mz-ordersummary-totalvalue">-{{ shippingDiscount.discount.impact|currency }}</span></td>
                        </tr>
                        {% endfor %}
                        {% if model.handlingTotal %}
                        <tr class="mz-ordersummary-additionalhandling">
                            <td><span class="mz-ordersummary-totalname">{{ labels.shippingSurcharge }}{% if model.handlingAmount > model.handlingTotal  %}<span class="is-crossedout" {% block crossedout-inline-style %}{% endblock crossedout-inline-style %}> ( {{ model.handlingAmount|currency }} )</span>{% endif %}</span></td>
                            <td><span class="mz-ordersummary-totalvalue">{{ model.handlingTotal|currency }}</span></td>
                        </tr>
                        {% endif %}
                        {% for discount in model.orderDiscounts %}
                        {% if not discount.excluded %}
                          <tr class="mz-ordersummary-discount">
                            <td><span class="mz-ordersummary-discountname">Discount </span></td>
                            <td><span class="mz-ordersummary-discountamt">-{{ discount.impact|currency }}</span></td>
                          </tr>
                        {% endif %}
                        {% endfor %}

                        {% if model.dutyTotal > 0 %}
                          <tr class="mz-ordersummary-tax">
                            <td><span class="mz-ordersummary-totalname">{{ labels.taxDuty }}</span></td>
                            <td><span class="mz-ordersummary-totalvalue">{% filter currency %} {{ model.dutyTotal|add(model.taxTotal)}} {% endfilter %}</span></td>
                          </tr>
                        {% else %}
                        {% if model.taxTotal %}
                          <tr class="mz-ordersummary-tax">
                            <td><span class="mz-ordersummary-totalname">{{ labels.tax }}</span></td>
                            <td><span class="mz-ordersummary-totalvalue">{{ model.taxTotal|currency }}</span></td>
                          </tr>
                        {% endif %}
                        {% endif %}

                        <tr class="mz-ordersummary-grandtotal">
                            <td><span class="mz-ordersummary-totalname">{{ labels.total }}</span></td>
                            <td><span class="mz-ordersummary-totalvalue">{{ model.total|currency }}</span></td>
                        </tr>
                        {% if model.billingInfo and model.billingInfo.digitalCreditPaymentTotal %}
                          <tr class="mz-ordersummary-digitalcreditpayment">
                            <td><span class="mz-ordersummary-totalname">{{ labels.digitalCreditTotal }}</span></td>
                            <td><span class="mz-ordersummary-totalvalue">{{ labels.digitalCreditSubtractFromOrderPrefix }}{{ model.billingInfo.digitalCreditPaymentTotal|currency }}{{ labels.digitalCreditSubtractFromOrderSuffix }}</span></td>
                          </tr>
                          <tr class="mz-ordersummary-grandtotal">
                              <td><span class="mz-ordersummary-totalname">{{ labels.remainingBalance }}:</span></td>
                              <td><span class="mz-ordersummary-totalvalue">{{ model.billingInfo.nonStoreCreditTotal|currency }}</span></td>
                          </tr>
                        {% endif %}

                    </tbody>
                </table>
            </div>
              <div class="help-sec">
               {% dropzone "confirmation-help-text" scope="template"%}
              </div>
            </div>
          </div>
       </div>
    </div>
    <div class="confiramtion-slider">
      {% dropzone "confriramtion-theme-slider" scope="template" %}
      <div data-mz-recommendedProducts id="recommended_products_slot">

      </div>
    </div>
</div>

{% if model.billingInfo.paymentType != "check"%} 
