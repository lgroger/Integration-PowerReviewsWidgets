                            </a>
                            {% if model.data.smcatnameD %}
                            <div class="contentContainer">
                                <a class="contentTitle" href="{% if category.content.slug %}/{{category.content.slug}}{% endif %}/c/{{category.categoryId}}">
                                    {{model.data.smcatnameD}}
                                </a>
                                <span class="arrow"></span>
                            </div>
                            {%endif%}
                        {% endif %}
                    {% endwith %}
                </div>
            </div>
            <div class="small-image">
                <div class="content">
                    {% with model.categories|findwhere("categoryId",model.data.smcatselectE) as category %}
                        {% if category %}
                            <a href="{% if category.content.slug %}/{{category.content.slug}}{% endif %}/c/{{category.categoryId}}">
