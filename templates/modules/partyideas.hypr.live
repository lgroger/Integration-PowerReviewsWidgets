                                {% endif %}
                            </a>
                            {% if model.data.smcatnameE %}
                                <div class="contentContainer">
                                    <a class="contentTitle" href="{% if category.content.slug %}/{{category.content.slug}}{% endif %}/c/{{category.categoryId}}">
                                        {{model.data.smcatnameE}}
                                    </a>
                                    <span class="arrow"></span>
                                </div>
                            {% endif %}
                        {% endif %}
                    {% endwith %}
                </div>
                <div class="content">
                    {% with model.categories|findwhere("categoryId",model.data.smcatselectF) as category %}
                        {% if category %}
                            <a href="{% if category.content.slug %}/{{category.content.slug}}{% endif %}/c/{{category.categoryId}}">
                                {% if model.data.smoverrideF %}
                                    <img src="{{model.data.smimageurlF}}" alt="{{model.data.smcatnameF}}"/>
                                {% else %}
                                    {% with category.content.categoryImages|first as catimage %}
                                        <img src="{{catimage.imageUrl}}" alt="{{model.data.smcatnameF}}"/>
                                    {% endwith %}
                                {% endif %}
                            </a>
                            {% if model.data.smcatnameF %}
                                <div class="contentContainer">
                                    <a class="contentTitle" href="{% if category.content.slug %}/{{category.content.slug}}{% endif %}/c/{{category.categoryId}}">
                                        {{model.data.smcatnameF}} 
                                    </a>
                                    <span class="arrow"></span>
                                </div>
                            {% endif %}
                        {% endif %}
                    {% endwith %}
                </div>
            </div> 
        </div>
    </div>
</div>

<!-- HTML for Party Ideas Widget --> 