<!-- HTML for Party Ideas Widget -->

<div class="LP-cat-pi">
    <div class="left">
        <div class="logoContainer">
            <div class="party">
            </div>
            {%if pageContext.isDesktop %}
                <span class="desc">Great ideas come from great party throwers like you</span>
            {%else%}
                <div class="horizontalLine"><hr/></div>
            {% endif %}
        </div>
        <div class="large-image">
            <div class="content">
                {% with model.categories|findwhere("categoryId",model.data.lgcatselectA) as category %}
                    {% if category %}
                        <a href="{% if category.content.slug %}/{{category.content.slug}}{% endif %}/c/{{category.categoryId}}">
                            {% if model.data.lgoverrideA %}
                                <img src="{{model.data.lgimageurlA}}" alt="{{model.data.lgcatnameA}}"/>
                            {% else %}
                                {% with category.content.categoryImages|first as catimage %}
                                    <img src="{{catimage.imageUrl}}" alt="{{model.data.lgcatnameA}}"/>
                                {% endwith %}
                            {% endif %}
                        </a>
                        {% if model.data.lgcatnameA %}
                            <div class="contentContainer">
                                <a class="contentTitle" href="{% if category.content.slug %}/{{category.content.slug}}{% endif %}/c/{{category.categoryId}}">
                                    {{model.data.lgcatnameA}}
                                </a> 
                                <span class="arrow"></span>
                            </div>
                        {% endif %}
                    {% endif %}
                {% endwith %} 
            </div>
             <div class="content">
                {% with model.categories|findwhere("categoryId",model.data.lgcatselectB) as category %}
                    {% if category %}
                        <a href="{% if category.content.slug %}/{{category.content.slug}}{% endif %}/c/{{category.categoryId}}">
                            {% if model.data.lgoverrideB %}
                                <img src="{{model.data.lgimageurlB}}" alt="{{model.data.lgcatnameB}}"/>
                            {% else %}
                                {% with category.content.categoryImages|first as catimage %}
                                    <img src="{{catimage.imageUrl}}" alt="{{model.data.lgcatnameB}}"/>
                                {% endwith %}
                            {% endif %}

                        </a>
                        {% if model.data.lgcatnameB %}
                        <div class="contentContainer">
                            <a class="contentTitle" href="{% if category.content.slug %}/{{category.content.slug}}{% endif %}/c/{{category.categoryId}}">
                                {{model.data.lgcatnameB}}
                            </a>
                            <span class="arrow"></span>
                        </div>
                        {%endif%}
                    {% endif %} 
                {% endwith %}
            </div>
        </div>
    </div>
    <div class="right">
        {%if pageContext.isDesktop %}
            <div class="horizontalLineContainer">
                <hr/>
            </div>
        {%endif%}
        <div class="smallImageContainer">
            <div class="small-image">
                <div class="content">
                    {% with model.categories|findwhere("categoryId",model.data.smcatselectC) as category %}
                        {% if category %}
                            <a href="{% if category.content.slug %}/{{category.content.slug}}{% endif %}/c/{{category.categoryId}}">
