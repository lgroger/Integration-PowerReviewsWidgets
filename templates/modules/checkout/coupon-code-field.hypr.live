								<option value="4075">Army National Guard</option>
								<option value="4076">Army Reserve</option>
								<option value="4074">Coast Guard</option>
								<option value="4081">Coast Guard Reserve</option>
								<option value="4071">Marine Corps</option>
								<option value="4077">Marine Corps Forces Reserve</option>
								<option value="8353">National Oceanic and Atmospheric Administration</option>
								<option value="4072">Navy</option>
								<option value="4078">Navy Reserve</option>
								<option value="4082">Other DoD</option>
